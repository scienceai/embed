{
  "name": "@scienceai/embed",
  "version": "1.0.0",
  "description": "makes trees out of flattened JSON-LD documents",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel-register",
    "prepublish": "rm -rf dist && ./node_modules/.bin/babel src --out-dir dist"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scienceai/embed.git"
  },
  "keywords": [
    "JSON-LD",
    "graph",
    "tree"
  ],
  "author": "Sebastien Ballesteros",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/scienceai/embed/issues"
  },
  "homepage": "https://github.com/scienceai/embed#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-register": "6.18.0",
    "jsonld": "0.4.11",
    "mocha": "3.1.2"
  }
}
